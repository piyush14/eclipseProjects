package com.enumdemo;

 enum Status{
	 NEW(1),
	 REJECTED(2),
	 ACCEPTED(3),
	 COMPLETED(4);
	 int value;
	 
	 private Status(int value){
		 this.value = value;
	 }
 };

public class EnumDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Order o1 = new Order(101, "fruit", "good", 500);
		Order o2 = new Order(121, "juice", "good", 230);

	}

}
