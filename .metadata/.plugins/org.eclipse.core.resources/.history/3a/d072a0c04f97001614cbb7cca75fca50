package readandwrite;

import java.io.File;
import java.io.FileInputStream;

public class ReadWrite {
	File file = new File("file1.txt");
	FileInputStream fis = new FileInputStream(file);
	StringBuilder sb = new StringBuilder("");
	int i = 0;
	do {
		i = fis.read();
	if(i != -1)
	sb.append((char)i);
	}
	while(i != -1);
}

