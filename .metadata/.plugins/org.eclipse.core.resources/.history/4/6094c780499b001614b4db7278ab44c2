package dateofbirth;

import java.util.HashMap;

public class HashMapMain {

	public static void main(String[] args) {
		
		HashMap<Date, String> emp = new HashMap<>();
		emp.put(new Date(1,1,2000), "ABC");
	}

}

class Date{
	public int date;
	public int month;
	public int year;
	
	public Date(int date,int month,int year){
		this.date = date;
		this.month = month;
		this.year = year;
	}
	
}
