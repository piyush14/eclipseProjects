package com.difftrycatch;

public class Trycatch {
	
	public void test(){
		int [] a = new int [3];
		try{
			
			int ans = 5 /0;
			try{
				a[4]=10;
			}catch(Exception e){
				System.out.println("Inner try block");
				
			}
		}catch(ArithmeticException e){
			e.printStackTrace();
		}
		catch (NullPointerException e) {
			e.printStackTrace(); 
		}
	}

}
